<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html"; charset=utf-8" />
    <title>{{project_name}} |{% block title %}{% endblock %}</title>
    {% block stylesheets %}
        <!-- Le styles -->

        <link href="{{ asset('/assets/css/bootstrap.css') }}" rel="stylesheet">
        <link href="{{ asset('/assets/css/bootstrap-responsive.css') }}" rel="stylesheet">
        <link href="{{ asset('/assets/css/docs.css') }}" rel="stylesheet">
        <!-- Le fav and touch icons -->
        <link rel="shortcut icon" href="assets/ico/favicon.ico">
        <link rel="apple-touch-icon-precomposed" sizes="144x144" href="assets/ico/apple-touch-icon-144-precomposed.png">
        <link rel="apple-touch-icon-precomposed" sizes="114x114" href="assets/ico/apple-touch-icon-114-precomposed.png">
        <link rel="apple-touch-icon-precomposed" sizes="72x72" href="assets/ico/apple-touch-icon-72-precomposed.png">
        <link rel="apple-touch-icon-precomposed" href="assets/ico/apple-touch-icon-57-precomposed.png">
    {% endblock %}
</head>
<body>
{% block navigation %}
    {% include '@site/layouts/front_navbar.html.twig'  %}
{% endblock %}
{% block body %}
    Page content
{% endblock %}
{% block footer %}
    {% include '@site/layouts/front_footer.html.twig'  %}
{% endblock %}
{% block javascript %}
    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    {#<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>#}
    {#<script src="{{ '/build/js/app.js' }}"></script>#}
    <script src="{{ asset('/assets/js/jquery.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-transition.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-alert.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-modal.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-dropdown.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-scrollspy.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-tab.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-tooltip.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-popover.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-button.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-collapse.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-carousel.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-typeahead.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/bootstrap-affix.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/application.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/superfish.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/js/custom.js') }}" type="text/javascript"></script>
{% endblock %}
</body>
</html>